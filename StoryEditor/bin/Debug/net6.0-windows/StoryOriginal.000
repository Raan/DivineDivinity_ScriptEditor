}

KB {
IF
InRegion(NPC_Hero, REGION_warriors_guild)
AND
ReadyToResumeParrying(NPC_Instructor)
AND
ReadyToResumeParrying(NPC_Trainee)
THEN
InstructorAndTraineeStartTraining(1);

IF
NpcLeavesRegion(NPC_Hero, REGION_warriors_guild)
THEN
InstructorAndTraineeStopTraining(1);

IF
NpcHitpointsCritical(NPC_Trainee)
THEN
SetHitpoints(NPC_Trainee, 50);

IF
NpcHitpointsCritical(NPC_Instructor)
AND NOT
InstructorAndPlayerAreTraining(1)
THEN
SetHitpoints(NPC_Instructor, 65);

PROC
PreStartDialog(NPC_Trainee, DIALOG_Trainee)
THEN
InstructorAndTraineeStopTraining(1);

PROC
PreStartDialog(NPC_Instructor, DIALOG_Instructor)
THEN
InstructorAndTraineeStopTraining(1);

PROC
PreStartDialog(NPC_Trainee, _)
AND
RanDialog(DIALOG_Trainee)
THEN
SetAndRememberDialogEvent(DIALOG_EVENT_instructor_and_trainee_fighting, 1);

IF
DialogEnds(DIALOG_Trainee)
AND NOT
InstructorAndPlayerAreTraining(1)
THEN
InstructorAndTraineeReturnToTraining(1);

IF
DialogEnds(DIALOG_Instructor)
AND NOT
DialogConditionTrue(DIALOG_EVENT_instructor_start_fight)
THEN
InstructorAndTraineeReturnToTraining(1);

IF
DialogEnds(DIALOG_Instructor)
AND
DialogConditionTrue(DIALOG_EVENT_instructor_start_fight)
THEN
SetAndRememberDialogEvent(DIALOG_EVENT_instructor_start_fight, 0);
SetAndRememberDialogEvent(DIALOG_EVENT_instructor_player_accepts_fight, 1);
InstructorAndPlayerStartTraining(1);

IF
NpcHitpointsCritical(NPC_Instructor)
AND
InstructorAndPlayerAreTraining(1)
AND
InstructorHealedHimself(1)
THEN
InstructorAndPlayerStopTraining(1);
SetAndStartDialog(NPC_Instructor, DIALOG_Instructor_Beaten);

IF
DialogEnds(DIALOG_Instructor_Beaten)
THEN
SetAndRememberDialogEvent(DIALOG_EVENT_instructor_player_completed_training, 1);
InstructorGivesWarriorSkill(1);
LaunchTimer(ENGINE_GAME, "instructor tim 1", 1200);

IF
TimerFinished(ENGINE_GAME, "instructor tim 1")
THEN
InstructorAndTraineeReturnToTraining(1);

IF
NpcHitpointsCritical(NPC_Instructor)
AND
InstructorAndPlayerAreTraining(1)
AND NOT
InstructorHealedHimself(1)
THEN
SetHitpoints(NPC_Instructor, 80);
InstructorHealedHimself(1);

IF
NpcLeavesRegion(NPC_Hero, REGION_fight_instructor_wguild)
AND
InstructorAndPlayerAreTraining(1)
THEN
InstructorAndPlayerStopTraining(1);
SetAndStartDialog(NPC_Instructor, DIALOG_Instructor_player_leaves);

IF
DialogEnds(DIALOG_Instructor_player_leaves)
THEN
InstructorAndTraineeReturnToTraining(1);

IF
NpcLeavesRegion(NPC_Hero, REGION_fight_instructor_wguild)
THEN
StartScriptFrame(NPC_Hero, "Hero mortal");

IF
NpcHitpointsCritical(NPC_Hero)
AND
InstructorAndPlayerAreTraining(1)
THEN
InstructorAndPlayerStopTraining(1);
SetAndStartDialog(NPC_Instructor, DIALOG_Instructor_Beats_Hero);

IF
DialogEnds(DIALOG_Instructor_Beats_Hero)
THEN
InstructorAndTraineeReturnToTraining(1);

PROC
InstructorAndTraineeStopTraining(1)
THEN
EndFight(NPC_Trainee, NPC_Instructor);
StartScriptFrame(NPC_Instructor, "instructor wait");
StartScriptFrame(NPC_Trainee, "trainee wait");
SetAndRememberDialogEvent(DIALOG_EVENT_instructor_and_trainee_fighting, 0);
NOT ReadyToResumeParrying(NPC_Trainee);
NOT ReadyToResumeParrying(NPC_Instructor);

PROC
InstructorAndTraineeStartTraining(1)
THEN
NOT InstructorAndPlayerAreTraining(1);
BeginFight(NPC_Trainee, NPC_Instructor);
SetAndRememberDialogEvent(DIALOG_EVENT_instructor_and_trainee_fighting, 1);

PROC
InstructorAndPlayerStartTraining(1)
THEN
InstructorAndPlayerAreTraining(1);
NOT NpcFollowsAlignment(NPC_Instructor, "Knight");
SetHitpoints(NPC_Instructor, 100);
NOT InstructorHealedHimself(1);
StartScriptFrame(NPC_Hero, "Hero immortal");
NOT ReadyToResumeParrying(NPC_Instructor);
Unintelligent(NPC_Instructor);
TraineeLeaves(1);

PROC
InstructorAndPlayerStopTraining(1)
THEN
NOT InstructorAndPlayerAreTraining(1);
StartScriptFrame(NPC_Hero, "Hero mortal");
BoundSetAttitude(NPC_Instructor, 0);
NOT Unintelligent(NPC_Instructor);
NpcFollowsAlignment(NPC_Instructor, "Knight");

PROC
InstructorAndTraineeReturnToTraining(1)
THEN
InstructorAndPlayerStopTraining(1);
TraineeReturnToFight(1);
StartScriptFrame(NPC_Instructor, "instructor to stps");

PROC
TraineeLeaves(1)
AND
Random(FUNCTION_Math, 2, _Var1)
THEN
TraineeLeaves_1(_Var1);

PROC
TraineeLeaves(_)
THEN
TraineeLeft(1);

PROC
TraineeLeaves_1(0)
AND NOT
TraineeIsOutside(1)
THEN
StartScriptFrame(NPC_Trainee, "trainee move to corner");

PROC
TraineeLeaves_1(1)
THEN
StartScriptFrame(NPC_Trainee, "trainee move outside");
TraineeIsOutside(1);

PROC
TraineeLeaves_1(_)
THEN
NOT ReadyToResumeParrying(NPC_Trainee);

IF
NpcScriptFrameFinished(NPC_Trainee, "trainee move inside")
THEN
NOT TraineeIsOutside(1);

PROC
TraineeReturnToFight(1)
AND
TraineeLeft(1)
AND
TraineeIsOutside(1)
THEN
StartScriptFrame(NPC_Trainee, "trainee move inside");

PROC
TraineeReturnToFight(1)
AND
TraineeLeft(1)
AND NOT
TraineeIsOutside(1)
THEN
StartScriptFrame(NPC_Trainee, "trainee to stps");

PROC
TraineeReturnToFight(1)
AND NOT
TraineeLeft(1)
THEN
ReadyToResumeParrying(NPC_Trainee);

IF
NpcScriptFrameFinished(NPC_Trainee, "trainee to stps")
THEN
ReadyToResumeParrying(NPC_Trainee);

IF
NpcScriptFrameFinished(NPC_Instructor, "instructor to stps")
THEN
ReadyToResumeParrying(NPC_Instructor);

PROC
InstructorGivesWarriorSkill(1)
AND
PlayerSkillData(ENGINE_GAME, "IncreasedDamage", _Var1, _, _)
AND
PlayerSkillData(ENGINE_GAME, "IncreasedDefense", _Var4, _, _)
AND
PlayerSkillData(ENGINE_GAME, "Berserk", _Var7, _, _)
THEN
InstructorGivesWarriorSkill("IncreasedDamage", _Var1);
InstructorGivesWarriorSkill("IncreasedDefense", _Var4);
InstructorGivesWarriorSkill("Berserk", _Var7);

PROC
InstructorGivesWarriorSkill(1)
AND
InstructorGivesWarriorSkill("IncreasedDamage", _Var1)
AND
InstructorGivesWarriorSkill("IncreasedDefense", _Var2)
AND
InstructorGivesWarriorSkill("Berserk", _Var3)
AND
_Var1 >= _Var2
AND
_Var1 >= _Var3
AND
_Var1 < 80
THEN
AddSkill(ENGINE_GAME, "IncreasedDamage");
InstructorGivesWarriorSkill_Done(1);

PROC

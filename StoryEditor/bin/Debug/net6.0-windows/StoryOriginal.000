Goal(190).Title("Guards die at Aleroth");
Goal(190) {
INIT {
}

KB {
IF
NpcEntersRegion(NPC_Hero, REGION_front_gate_alertoh)
AND NOT
DidFightingScene(NPC_Seth)
THEN
StartWarSceneAleroth(1);

IF
NpcLeavesRegion(NPC_Hero, REGION_aleroth_forest_entry_catcs)
AND NOT
InRegion(NPC_Hero, REGION_Lady_Statue_Zombie_Exist)
AND NOT
DidFightingScene(NPC_Seth)
THEN
StartWarSceneAleroth(2);

PROC
StartWarSceneAleroth((INTEGER)_Var1)
THEN
WarSceneAleroth(_Var1);
StartScene(56);

PROC
DoStartScene(56)
THEN
taart();

PROC
DoStartScene(56)
THEN
Hold(NPC_Hero);
SetStoryNpc(NPC_Hero, 1);
StartScriptFrame(NPC_Dummy, "Dummy to aleroth warscene");
SetBuryEggsFlag(ENGINE_GAME, 0);
HatchMonsterEggs(REGION_aleroth_war_scene);
DidFightingScene(NPC_Seth);

PROC
DoStartScene(56)
AND
WarSceneAleroth(2)
THEN
DoRemoveNpc(NPC_Seth);

IF
NpcScriptFrameFinished(NPC_Dummy, "Dummy to aleroth warscene")
THEN
StartScriptFrame(NPC_Soldier1, "Soldier 1 easy target");
StartScriptFrame(NPC_Soldier2, "Soldier 2 easy target");
StartScriptFrame(NPC_Soldier3, "Soldier 3 easy target");
InterfaceDisabled(ENGINE_GAME, 1);
SetLineOfSightClipping(ENGINE_GAME, 0);
LockOnNpc(ENGINE_GAME, NPC_Dummy);
SetRenderShroud(ENGINE_GAME, 0);
LaunchTimer(ENGINE_GAME, "Guards die at Aleroth 1", 240);
LaunchTimer(ENGINE_GAME, "Guards die at Aleroth Guard 1 dies", 80);
LaunchTimer(ENGINE_GAME, "Guards die at Aleroth Guard 2 dies", 160);
LaunchTimer(ENGINE_GAME, "Guards die at Aleroth Guard 3 dies", 200);
LockStarted(16);

PROC
EndLock(16)
THEN
SetBuryEggsFlag(ENGINE_GAME, 1);
EndScene(56);
GoalCompleted;

IF
TimerFinished(ENGINE_GAME, "Guards die at Aleroth Guard 1 dies")
THEN
Kill(NPC_Soldier1);
Dead(NPC_Soldier1);

IF
TimerFinished(ENGINE_GAME, "Guards die at Aleroth Guard 2 dies")
THEN
Kill(NPC_Soldier2);
Dead(NPC_Soldier2);

IF
TimerFinished(ENGINE_GAME, "Guards die at Aleroth Guard 3 dies")
THEN
Kill(NPC_Soldier3);
Dead(NPC_Soldier3);

IF
TimerFinished(ENGINE_GAME, "Guards die at Aleroth 1")
AND
WarSceneAleroth(1)
THEN
ShiftToNpc(NPC_Seth);
StartScriptFrame(NPC_Seth, "Seth goto gate Aleroth");

IF
NpcScriptFrameFinished(NPC_Seth, "Seth goto gate Aleroth")
THEN
SetStoryNpc(NPC_Seth, 0);
StartScriptFrame(NPC_Seth, "Seth kill the orcs");
StartScriptFrame(NPC_sethkiller, "sethkiller attack Seth");
StartScriptFrame(NPC_Timm, "timm attack Seth");

IF
NpcScriptFrameFinished(NPC_Seth, "Seth kill the orcs")
THEN
SetFightMode(NPC_Seth, 1);
SetStoryNpc(NPC_Seth, 1);
SetHitpoints(NPC_Seth, 80);
StartScriptFrame(NPC_Seth, "Seth to hero");
SetBuryEggsFlag(ENGINE_GAME, 1);

IF
TimerFinished(ENGINE_GAME, "Guards die at Aleroth 1")
AND
WarSceneAleroth(2)
THEN
AppearOutOfSight(NPC_Seth, NPC_Hero, 315.0);
SetFightMode(NPC_Seth, 1);
SetStoryNpc(NPC_Seth, 1);
StartScriptFrame(NPC_Seth, "Seth to hero");
SetBuryEggsFlag(ENGINE_GAME, 1);
ShiftToNpc(NPC_Seth);
KillMonstersInRegion(REGION_aleroth_outside_gate);

IF
NpcScriptFrameFinished(NPC_Seth, "Seth to hero")
AND
WarSceneAleroth(1)
THEN
SetRenderShroud(ENGINE_GAME, 1);
SetAndStartDialog(NPC_Seth, DIALOG_Seth_Outside);

IF
DialogEnds(DIALOG_Seth_Outside, _, NPC_Seth)
THEN
SethAndHeroReturnToAleroth(1);

IF
NpcScriptFrameFinished(NPC_Seth, "Seth to hero")
AND
WarSceneAleroth(2)
THEN
SetRenderShroud(ENGINE_GAME, 1);
SetAndStartDialog(NPC_Seth, DIALOG_Seth_Outside_2);

IF
DialogEnds(DIALOG_Seth_Outside_2, _, _)
THEN
ShiftToNpc(NPC_Hero, 0);
InterfaceDisabled(ENGINE_GAME, 1);
StartScriptFrame(NPC_Seth, "Seth enters Aleroth");
StartScriptFrame(NPC_Hero, "Hero follow Seth");
LaunchTimer(ENGINE_GAME, "Guards die at Aleroth tim1", 120);

IF
TimerFinished(ENGINE_GAME, "Guards die at Aleroth tim1")
THEN
FadeBlack(ENGINE_GAME, 2);
LaunchTimer(ENGINE_GAME, "Guards die at Aleroth tim2", 20);

IF
TimerFinished(ENGINE_GAME, "Guards die at Aleroth tim2")
THEN
SetLocation(NPC_Hero, LOCATION_sethnhero1);
SetLocation(NPC_Seth, LOCATION_sethnhero2);
SethAndHeroReturnToAleroth(1);

PROC
SethAndHeroReturnToAleroth(1)
THEN
ShiftToNpc(NPC_Hero, 0);
InterfaceDisabled(ENGINE_GAME, 1);
StartScriptFrame(NPC_Seth, "Seth enters Aleroth");
StartScriptFrame(NPC_Hero, "Hero enters Aleroth");

IF
NpcScriptFrameFinished(NPC_Hero, "Hero enters Aleroth")
THEN
OK(1);

IF
NpcScriptFrameFinished(NPC_Seth, "Seth enters Aleroth")
THEN
StartScriptFrame(NPC_Seth, "Seth can die");

IF
NpcScriptFrameFinished(NPC_Seth, "Seth can die")
THEN
OK(2);

IF
OK(1)
AND
OK(2)
THEN
Close(OBJECT_Aleroth_gate1);
Close(OBJECT_Aleroth_gate2);
ShiftToNpc(NPC_Hero, 0);
StoryNpc(NPC_Hero, 0);
NOT OK(1);
NOT OK(2);
SetAndStartDialog(NPC_Seth);
EndScene(56);
GoalCompleted;

PROC
PROC_CompleteGoal("Guards die at Aleroth")
THEN
GoalCompleted;

}

EXIT {
SetStoryNpc(NPC_Seth, 0);
SetStoryNpc(NPC_Hero, 0);
StartScriptFrame(NPC_Seth, "Seth behave");
Kill(NPC_Soldier1);
Kill(NPC_Soldier2);
Kill(NPC_Soldier3);
NOT LockStarted(16);
}

}


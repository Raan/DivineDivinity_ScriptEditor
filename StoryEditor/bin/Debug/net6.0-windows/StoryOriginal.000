Goal(213).Title("Instructor");
Goal(213) {
INIT {
ReadyToResumeParrying(NPC_Instructor);
ReadyToResumeParrying(NPC_Trainee);
SetDefaultThreatenedDialog(NPC_Instructor, DIALOG_Instructor);
SetDefaultThreatenedDialog(NPC_Trainee, DIALOG_Trainee);

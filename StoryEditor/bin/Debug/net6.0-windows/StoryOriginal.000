SetAndRememberDialogEvent(DIALOG_EVENT_sword_hero_has_not_sword_in_the_stone, 0);

PROC
PreStartDialog(_, DIALOG_Talaryn)
AND NOT
ObjectInInventory(NPC_Hero, OBJECT_Z_Sword_Stone_Sword, 1)
AND
DialogConditionTrue(DIALOG_EVENT_sword_stone_ghost_freed)
AND NOT
DialogConditionTrue(DIALOG_EVENT_sword_stone_trapped_again)
THEN
SetAndRememberDialogEvent(DIALOG_EVENT_sword_hero_has_not_sword_in_the_stone, 1);
SetAndRememberDialogEvent(DIALOG_EVENT_sword_hero_has_sword_in_the_stone, 0);

IF
NpcSees(NPC_Hero, NPC_Talarin)
AND
Triggered("Talaryn region")
AND NOT
Triggered("Talaryn talks")
AND NOT
IsHostile(NPC_Talarin)
THEN
Triggered("Talaryn talks");
InterfaceDisabled(ENGINE_GAME, 1);
SetStoryNpc(NPC_Hero, 1);
SetStoryNpc(NPC_Talarin, 1);
StartScene(219);

PROC
DoStartScene(219)
THEN
CameraToLocation(ENGINE_GAME, LOCATION_stps_talarin, "DF Camp: Talarin", 10);

IF
CameraEvent(ENGINE_GAME, "DF Camp: Talarin")
THEN
ShiftToNpc(NPC_Talarin);
SetDefaultDialog(NPC_Talarin, DIALOG_Talaryn, 1);
SetAndStartDialog(NPC_Talarin, DIALOG_Talaryn);

IF
DialogEnds(DIALOG_Talaryn)
THEN
CameraToNpc(ENGINE_GAME, NPC_Hero, "DF Camp Talarin: Hero", 10);

IF
CameraEvent(ENGINE_GAME, "DF Camp Talarin: Hero")
AND
DialogConditionTrue(DIALOG_EVENT_talaryn_fights)
THEN
InterfaceDisabled(ENGINE_GAME, 0);
SetStoryNpc(NPC_Hero, 0);
SetStoryNpc(NPC_Talarin, 0);
EndScene(219);
ShiftToNpc(NPC_Hero);
Attack(NPC_Talarin, NPC_Hero);

IF
CameraEvent(ENGINE_GAME, "DF Camp Talarin: Hero")
AND NOT
DialogConditionTrue(DIALOG_EVENT_talaryn_fights)
THEN
LockOnNpc(ENGINE_GAME, NPC_Hero);
StartScriptFrame(NPC_Talarin, "Talaryn leaves");
LaunchTimer(ENGINE_GAME, "Talaryn leaves timer", 150);

IF
TimerFinished(ENGINE_GAME, "Talaryn leaves timer")
THEN
InterfaceDisabled(ENGINE_GAME, 0);
SetStoryNpc(NPC_Hero, 0);
SetStoryNpc(NPC_Talarin, 0);
EndScene(219);
ShiftToNpc(NPC_Hero);
DoRemoveNpc(NPC_Talarin, 0);
GoalCompleted;

}

EXIT {
}

}


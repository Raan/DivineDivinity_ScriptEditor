Goal(300).Title("Nurendel");
Goal(300) {
INIT {
}

KB {
IF
Dead(NPC_Nurendel)
THEN
GoalCompleted;

IF
NpcEntersDialogDistance(NPC_Hero, NPC_Nurendel)
AND NOT
DialogConditionTrue(DIALOG_EVENT_nurendel_attacks_hero)
AND NOT
Triggered("Nurendel talks")
AND NOT
IsHostile(NPC_Nurendel)
THEN
Triggered("Nurendel talks");
SetStoryNpc(NPC_Hero, 1);
SetStoryNpc(NPC_Nurendel, 1);
SetRenderShroud(ENGINE_GAME, 0);
SetLineOfSightClipping(ENGINE_GAME, 0);
ShiftToNpc(NPC_Nurendel);
SetAndStartDialog(NPC_Nurendel, DIALOG_Nurendel);

IF
DialogEnds(DIALOG_Nurendel)
AND
IsStoryNpc(NPC_Hero)
AND NOT
DialogConditionTrue(DIALOG_EVENT_nurendel_attacks_hero)
THEN
SetRenderShroud(ENGINE_GAME, 1);
SetLineOfSightClipping(ENGINE_GAME, 1);
ShiftToNpc(NPC_Hero);
SetStoryNpc(NPC_Hero, 0);
SetStoryNpc(NPC_Nurendel, 0);
NOT Triggered("Nurendel talks");

IF
DialogEnds(DIALOG_Nurendel)
AND
DialogConditionTrue(DIALOG_EVENT_nurendel_attacks_hero)
AND NOT
Triggered("Nurendel attacks Hero")
THEN
Triggered("Nurendel attacks Hero");
SetRenderShroud(ENGINE_GAME, 1);
SetLineOfSightClipping(ENGINE_GAME, 1);
ShiftToNpc(NPC_Hero);
SetStoryNpc(NPC_Hero, 0);
NpcCannotCallGuards(NPC_Nurendel);
BeginFight(NPC_Nurendel, NPC_Hero);

IF
NpcLeavesRegion(NPC_Hero, REGION_Nurendel_Region)
AND
Triggered("Nurendel attacks Hero")
THEN
NOT Triggered("Nurendel attacks Hero");
EndFight(NPC_Nurendel, NPC_Hero);
StartScriptFrame(NPC_Nurendel, "Nurendel moves back");

IF
NpcScriptFrameFinished(NPC_Nurendel, "Nurendel moves back")
THEN
SetFightMode(NPC_Nurendel, 0);

IF
NpcEntersRegion(NPC_Hero, REGION_Nurendel_Region)
AND
DialogConditionTrue(DIALOG_EVENT_nurendel_attacks_hero)
AND NOT
Triggered("Nurendel attacks Hero")
THEN
Triggered("Nurendel attacks Hero");
NpcCannotCallGuards(NPC_Nurendel);
BeginFight(NPC_Nurendel, NPC_Hero);

IF
NpcLeavesRegion(NPC_Hero, REGION_Nurendel_Region)
AND
DialogConditionTrue(DIALOG_EVENT_nurendel3)
THEN
Lock_Direct(OBJECT_Z_Nurendel_Door);

IF
NpcLeavesRegion(NPC_Hero, REGION_Verdistis_Duchal_Inn_Room)
AND
Dead(NPC_Nurendel)
THEN
Lock(OBJECT_Z_Nurendel_Door);

}

EXIT {
}

}


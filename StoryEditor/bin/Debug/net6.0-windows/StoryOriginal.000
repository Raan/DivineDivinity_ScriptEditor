Goal(107).Title("DH_Three_Dwarves");
Goal(107) {
INIT {
SetStoryNpc(NPC_DH_DwarfTalk1, 1);
SetStoryNpc(NPC_DH_DwarfTalk2, 1);
SetStoryNpc(NPC_DH_DwarfTalk3, 1);
}

KB {
IF
Dead(NPC_DH_DwarfTalk1)
THEN
GoalCompleted;

IF
Dead(NPC_DH_DwarfTalk2)
THEN
GoalCompleted;

IF
Dead(NPC_DH_DwarfTalk3)
THEN
GoalCompleted;

IF
NpcEntersRegion(NPC_Hero, REGION_DH_Three_Dwarves)
AND NOT
Triggered("DH 3D talk")
AND NOT
Dead(NPC_DH_DwarfTalk1)
AND NOT
Dead(NPC_DH_DwarfTalk2)
AND NOT
Dead(NPC_DH_DwarfTalk3)
AND NOT
IsHostile(NPC_DH_DwarfTalk1)
AND NOT
IsHostile(NPC_DH_DwarfTalk2)
AND NOT
IsHostile(NPC_DH_DwarfTalk3)
THEN
InterfaceDisabled(ENGINE_GAME, 1);
SetStoryNpc(NPC_Hero, 1);
StartScene(121);

PROC
DoStartScene(121)
THEN
CameraToLocation(ENGINE_GAME, LOCATION_stps_Dwarf_Talk3, "DH three dwarves: DwarfTalk3", 10);

IF
CameraEvent(ENGINE_GAME, "DH three dwarves: DwarfTalk3")
THEN
ShiftToNpc(NPC_DH_DwarfTalk3);
StartMonologue(ENGINE_GAME, "MONO_DH_Dwarf_Talk");

IF
MonologueEvent(ENGINE_GAME, "MONO_DH_Dwarf_Talk Ends")
AND NOT
Triggered("DH 3D talk")
THEN
Triggered("DH 3D talk");
CameraToNpc(ENGINE_GAME, NPC_Hero, "DH three dwarves: Hero", 10);

IF
CameraEvent(ENGINE_GAME, "DH three dwarves: Hero")
THEN
InterfaceDisabled(ENGINE_GAME, 0);
SetStoryNpc(NPC_DH_DwarfTalk1, 0);
SetStoryNpc(NPC_DH_DwarfTalk2, 0);
SetStoryNpc(NPC_DH_DwarfTalk3, 0);
SetStoryNpc(NPC_Hero, 0);
EndScene(121);
ShiftToNpc(NPC_Hero);
StartScriptFrame(NPC_DH_DwarfTalk1, "DwarfTalk1 behave");
StartScriptFrame(NPC_DH_DwarfTalk2, "DwarfTalk2 behave");
StartScriptFrame(NPC_DH_DwarfTalk3, "DwarfTalk3 behave");

}

EXIT {
}

}

